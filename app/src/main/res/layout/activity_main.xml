        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/akvarium" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="255dp"
        android:layout_height="214dp"
        app:layout_constraintEnd_toEndOf="parent"
